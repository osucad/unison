{
  "name": "@unison/unison-server",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "serve": {
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "@unison/unison-server:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "@unison/unison-server:build:development"
          },
          "production": {
            "buildTarget": "@unison/unison-server:build:production"
          }
        }
      }
    }
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.9",
    "@unison/protocol": "workspace:^",
    "cors": "^2.8.5",
    "eventemitter3": "^5.0.1",
    "jsonwebtoken": "^9.0.2",
    "neverthrow": "^8.2.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17"
  }
}
